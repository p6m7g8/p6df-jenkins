#!/usr/bin/env groovy

pipeline {
  agent any

  options {
    disableConcurrentBuilds()
   }

   stages {
     stage('Stage 1') {
       steps {
         sh 'uname -a'
         sh 'df -h'
         sh 'env'
         sh 'echo $PATH'
         sh 'find /'
         sh 'whoami'
         sh 'ifconfig -a'
       }
     }
   }
}
